<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import com.dasa.challenge.labapp.controllers.components.ProcedureButton?>
<?import javafx.scene.text.TextFlow?>
<fx:root type="VBox" xmlns:fx="http://javafx.com/fxml" styleClass="procedureContainer" maxWidth="240.0"
         maxHeight="250.0">
    <VBox styleClass="procedureHeader">
        <TextFlow>
            <Text fx:id="procedureTitle" styleClass="procedureTitle"/>
        </TextFlow>
        <!--        <Text fx:id="procedureId" styleClass="procedureId"/>-->
    </VBox>
    <VBox styleClass="procedureBody">
        <TextFlow>
            <Text fx:id="procedureItems" styleClass="procedureItems"/>
        </TextFlow>
        <TextFlow>
            <Text fx:id="procedureDescription" styleClass="procedureDescription"/>
        </TextFlow>
        <ProcedureButton fx:id="procedureButton" alignment="CENTER" styleClass="procedureButton"/>
    </VBox>
</fx:root>